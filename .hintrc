{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": "off",
    "axe/structure": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]",
          "iframe[loading]"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 106",
    "not firefox <= 106",
    "not ios_saf <= 16.1",
    "not safari <= 16.1"
  ]
}